<form action="/participant/create" method="POST">
  <div class="form-group">
    <label for="">Masukkan No. KTP</label>
    <input type="text" class="form-control" id="idCard" name="idCard">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Masukkan Nama</label>
    <input type="text" class="form-control" id="name" name="name">
  </div>
  <div class="form-group">
    <label for="exampleFormControlSelect1">Pilih Jenis Kelamin</label>
    <select class="form-control" id="exampleFormControlSelect1" name="gender" required>
      <option>---Pilihan Jenis Kelamin---</option>
      <option value="laki - laki">Laki - laki</option>
       <option value="perempuan">Perempuan</option>
    </select>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Masukkan Alamat</label>
    <input type="text" class="form-control" id="address" name="address">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Masukkan Nomor Telepon</label>
    <input type="number" class="form-control" id="phoneNumber" name="phoneNumber">
  </div>

  <button type="submit" class="btn btn-primary">Submit</button>
</form>